{
  "name": "@delmaredigital/payload-page-tree",
  "version": "0.3.8",
  "description": "Payload CMS plugin for folder-based hierarchical URL slugs with visual tree management",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./client": {
      "import": "./dist/exports/client.js",
      "types": "./dist/exports/client.d.ts",
      "default": "./dist/exports/client.js"
    },
    "./rsc": {
      "import": "./dist/exports/rsc.js",
      "types": "./dist/exports/rsc.d.ts",
      "default": "./dist/exports/rsc.js"
    },
    "./theme.css": "./dist/styles/theme.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:swc && pnpm build:types && pnpm build:styles",
    "build:swc": "swc ./src -d ./dist --strip-leading-paths",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "build:styles": "copyfiles -u 1 \"src/**/*.css\" dist",
    "dev": "swc ./src -d ./dist --strip-leading-paths --watch",
    "dev-publish": "pnpm build && pnpm version prerelease --preid=dev --no-git-tag-version && pnpm publish --registry http://localhost:4873 --no-git-checks",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "@payloadcms/next": ">=3.0.0",
    "@payloadcms/ui": ">=3.0.0",
    "next": ">=14.0.0 || >=15.0.0 || >=16.0.0",
    "payload": ">=3.0.0",
    "react": ">=18.0.0 || >=19.0.0"
  },
  "peerDependenciesMeta": {
    "@payloadcms/ui": {
      "optional": true
    },
    "@payloadcms/next": {
      "optional": true
    }
  },
  "dependencies": {
    "react-arborist": "^3.4.3",
    "sonner": "^2.0.7"
  },
  "devDependencies": {
    "@payloadcms/next": "^3.73.0",
    "@payloadcms/ui": "^3.73.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.15.11",
    "@types/node": "^22.19.7",
    "@types/react": "^19.2.10",
    "copyfiles": "^2.4.1",
    "next": "^16.1.6",
    "payload": "^3.73.0",
    "react": "^19.2.4",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "payload",
    "payloadcms",
    "payload-plugin",
    "page-tree",
    "folders",
    "slugs",
    "hierarchical",
    "url",
    "pages",
    "cms",
    "tree-view"
  ],
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "author": "Delmare Digital",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delmaredigital/payload-page-tree.git"
  },
  "bugs": {
    "url": "https://github.com/delmaredigital/payload-page-tree/issues"
  },
  "homepage": "https://delmaredigital.com/developers"
}
